  "publisher": "marceloxp",
  "icon": "images/icon.png",
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "marceloxp",
    "url": "https://github.com/marceloxp"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Snippets",
